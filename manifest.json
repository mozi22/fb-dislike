{
  "manifest_version": 2,
  "name": "dislike",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js","parse_sdk.js","constants.js","content.js","parse.js"]
    }
  ],
  "browser_action": {
    "default_icon": "dislike.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
      "tabs", "http://*/*", "https://*/*"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ],
  "content_security_policy": "script-src 'self' https://connect.facebook.net https://graph.facebook.com https://www.parsecdn.com/js/parse-1.6.7.min.js; object-src 'self'"
}